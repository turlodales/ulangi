    "joi-react-native": "^3.0.0",
    "jsc-android": "^245459.0.0",
    "lodash": "^4.17.4",
    "lodash-decorators": "^6.0.1",
    "mobx": "^5.9.4",
    "mobx-react": "^5.0.0",
    "moment": "^2.22.1",
    "path-browserify": "0.0.0",
    "react": "^16.9.0",
