{
  "private": true,
  "name": "@ulangi/ulangi-mobile",
  "version": "3.11.4",
  "scripts": {
    "postbootstrap": "cd ios && pod install",
    "android": "react-native run-android --variant devDebug --appIdSuffix dev",
    "ios": "react-native run-ios --scheme UlangiMobile-Dev",
    "start": "react-native start",
    "version": "./scripts/sync-app-version.sh && git add ios/Info.plist android/app/build.gradle",
    "pack-local": "./scripts/pack-local.sh",
    "install-packed-local": "./scripts/install-packed-local.sh",
    "install-local": "npm run pack-local && npm run install-packed-local",
    "lint": "eslint --ext .ts,.tsx src/",
    "lint-fix": "eslint --fix --ext .ts,.tsx src/",
    "clean": "rm -Rf ./artifacts tsconfig.tsbuildinfo",
    "compile": "tsc -noUnusedLocals -noUnusedParameters -p ./",
    "build": "npm run clean && npm run prettify && npm run lint-fix && npm run compile",
    "test": "jest --coverage",
    "watch": "gulp watch",
    "prettify": "prettier --write \"src/**/*.{ts,tsx}\""
  },
  "dependencies": {
    "@react-native-community/audio-toolkit": "^2.0.2",
