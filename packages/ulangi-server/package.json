    "popper.js": "^1.14.6",
    "pug": "^2.0.3",
    "query-string": "^6.1.0",
    "stopword": "^0.3.4"
  },
  "devDependencies": {
    "@types/app-root-path": "^1.2.4",
    "@types/bcrypt": "^2.0.0",
    "@types/cors": "^2.8.4",
    "@types/elasticsearch": "^5.0.23",
    "@types/express": "^4.17.0",
    "@types/google-cloud__text-to-speech": "^0.5.0",
    "@types/hashids": "^1.0.30",
    "@types/in-app-purchase": "^1.10.0",
    "@types/jest": "^23.3.10",
    "@types/joi": "^13.0.8",
    "@types/js-yaml": "^3.11.1",
    "@types/jsonfile": "^4.0.1",
    "@types/knex": "^0.15.1",
