{
  "private": true,
  "name": "@ulangi/ulangi-server",
  "version": "3.11.5",
  "description": "Ulangi Server",
  "main": "dist/index.js",
  "scripts": {
    "pack-local": "./scripts/pack-local.sh",
    "install-packed-local": "./scripts/install-packed-local.sh",
    "install-local": "npm run pack-local && npm run install-packed-local",
    "lint": "eslint --ext .ts src/",
    "lint-fix": "eslint --fix --ext .ts src/",
    "clean": "rm -rf ./dist",
    "compile": "tsc -noUnusedLocals -noUnusedParameters -p ./",
    "build": "npm run prettify && npm run lint-fix && npm run clean && npm run compile && npm run build-webpack",
    "build-dev": "npm run build && ./scripts/build-dev.sh",
    "build-test": "./scripts/build-test.sh",
    "build-webpack": "webpack --mode=production",
    "start": "node dist/index.js",
    "dev": "env $(cat ./config/.env.dev | grep -v \"#\" | xargs) npm start",
    "start-dev": "./scripts/start-dev.sh",
    "test": "npm run test-unit && npm run test-e2e",
    "test-e2e": "./scripts/test-e2e.sh",
    "test-unit": "./scripts/test-unit.sh",
    "prettify": "prettier --write \"src/**/*.ts\""
  },
  "author": "Minh Loi",
  "license": "GPL-3.0",
  "dependencies": {
    "@google-cloud/text-to-speech": "^1.1.4",
    "@google-cloud/translate": "^2.1.2",
    "@ulangi/assert": "^1.0.6",
    "@ulangi/resolver": "^2.0.3",
    "@ulangi/wiktionary-core": "^2.2.1",
    "app-root-path": "^2.0.1",
    "autobind-decorator": "^2.1.0",
    "aws-sdk": "^2.242.1",
    "axios": "^0.18.0",
    "bcrypt": "^3.0.4",
    "body-parser": "^1.18.3",
    "bootstrap": "^4.2.1",
    "chalk": "^2.4.1",
    "change-case": "^3.0.2",
    "cors": "^2.8.4",
    "elasticsearch": "^15.0.0",
    "express": "^4.17.1",
    "firebase-admin": "^8.3.0",
    "in-app-purchase": "^1.11.4",
    "joi": "^14.3.1",
    "jquery": "^3.3.1",
    "js-yaml": "^3.12.0",
    "jsonfile": "^4.0.0",
    "knex": "^0.16.3",
    "lodash": "^4.17.11",
    "log-symbols": "^3.0.0",
    "moment": "^2.22.2",
    "mysql": "^2.16.0",
    "mysql2": "^1.6.5",
    "nodemailer": "^4.6.7",
    "passport": "^0.4.0",
    "passport-http-bearer": "^1.0.1",
    "passport-jwt": "^4.0.0",
